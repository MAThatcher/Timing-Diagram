import java.util.*;
public class SRT{
  private Task[] tasks;
  private int time = 0;
  private String[][] output;
  private Task[] order = new Task[20];
  
  //our constructor
  public SRT (Task[] ourProcesses){
    
  }

    

  public void showOutput()
  {
    for(int i = output.length -1 ; i >= 0; i--)
    {
      for (int j = 0 ; j < output[i].length ; j++)
      {
        System.out.print(output[i][j] + " ");
      }
      System.out.println("");
    }
  }
  
}